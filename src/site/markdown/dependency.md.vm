${h2} Dependency Management

${h3} Maven

		<dependency>
			<groupId>io.earcam</groupId>
			<artifactId>io.earcam.unexceptional</artifactId>
			<version>${project.version}</version>
		</dependency>

${h3} Gradle

		compile group: 'io.earcam', name: 'io.earcam.unexceptional', version: '${project.version}'

${h3} Ivy

		<dependency org="io.earcam" name="io.earcam.unexceptional" rev="${project.version}" />
		
${h3} SBT

		libraryDependencies += "io.earcam" % "io.earcam.unexceptional" % "${project.version}"

